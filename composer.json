{
    "name": "hussainweb/drupal-stats",
    "description": "Drupal Statistics Generator.",
    "keywords": ["laravel", "drupal"],
    "license": "GPL-2.0",
    "type": "project",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "johan/worldjson",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/johan/world.geo.json/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "hussainweb/drupal-api-client": "~1.0",
        "jenssegers/mongodb": "^3.4",
        "johan/worldjson": "^1.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "league/fractal": "^0.14.0",
        "michelf/php-markdown": "^1.6",
        "pda/pheanstalk": "~3.0",
        "pmatseykanets/artisan-beans": "^0.1.4"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "platform": {
            "ext-mongodb": "1.5.0"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
